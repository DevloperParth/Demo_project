<%= stylesheet_link_tag "https://maxcdn.bootstrapcdn.com/bootstrap/3.4.1/css/bootstrap.min.css" %>
<table class="table" >
  <tr>
    <th>id</th>
    <th>project title</th>
    <th>Client Name</th>
    <th>PayRate</th>
    <th>Action</th>
  </tr>
<% @project.each do |project| %>
  <tr>
    <td><%= project.id %></td>
    <td><%= project.title %></td>
    <td><%= project.client_name %></td>
     <td><%= project.pay_rate %></td>
    <td><%= link_to "ADD USER", save_assginment_assginedprojects_path({user_id: params[:user], project_id: project.id}), method: :post %></td>
  </tr>
<% end %>





